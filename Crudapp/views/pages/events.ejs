<div class="jumbotron text-center">
    <h1>All Events</h1>
</div>

<table class="table table-bordered table-hover table-striped">
    <thead>
        <tr>
            <th>Event Name</th>
            <th>Description</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% for (var event of events ) { %>
            <tr>
                <td>
                    <%= events.name %>
                </td>
                <td>
                    <%= events.description %>
                </td>
                <td>
                    <a href="/events<%= event.slug %>" class="btn btn-sm btn-primary">View</a>
                </td>
            </tr>
            <% } %>
    </tbody>
</table>